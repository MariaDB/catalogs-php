    "type": "library",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "Mariadb\\CatalogsPHP\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*",
        "squizlabs/php_codesniffer": "3.*"
